from turtle import *
import random
t = Turtle()
    
pen_colors = ['red', 'orange', 'yellow', 'green', \
'blue', 'purple', 'pink', 'silver', 'violet', 'turquoise']



for i in range(20):
    num1=random.randint(1,105)
    t.pencolor(random.choice(pen_colors))
    t.goto(0, 0) 
    t.forward(num1)
    t.right(num1)
    
    
t.clear() 
